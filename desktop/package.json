{
  "name": "jarvis-desktop",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "tauri": "tauri",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:tauri": "wdio run wdio.tauri.conf.ts",
    "test:tauri:build": "pnpm tauri build -- --debug && pnpm test:tauri",
    "test:ai": "playwright test --config=playwright.testdriver.config.ts",
    "test:ai:ui": "playwright test --config=playwright.testdriver.config.ts --ui",
    "test:ai:headed": "playwright test --config=playwright.testdriver.config.ts --headed"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tauri-apps/cli": "^2.0.0",
    "@tsconfig/svelte": "^5.0.2",
    "@wdio/cli": "^9.23.3",
    "@wdio/local-runner": "^9.23.3",
    "@wdio/mocha-framework": "^9.23.3",
    "@wdio/spec-reporter": "^9.23.3",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^6.0.0",
    "webdriverio": "^9.23.3"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.0",
    "@tauri-apps/plugin-sql": "^2.3.1",
    "@testdriver.ai/playwright": "^1.0.5"
  }
}
